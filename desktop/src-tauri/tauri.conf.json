{
  "productName": "Synergetica",
  "version": "1.0.0",
  "identifier": "dev.khokao.synergetica",
  "build": {
    "beforeBuildCommand": "npm run next-build",
    "beforeDevCommand": "npm run next-dev",
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 768,
        "minHeight": 768,
        "width": 1024,
        "minWidth": 1024,
        "resizable": true,
        "title": "Synergetica",
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
